<Project Sdk="Microsoft.NET.Sdk.Razor">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
	  <PackageId>AspNetCore.Authentication.SK.IDCard</PackageId>
	  <Version>1.0.0-alpha1</Version>
	  <PackageDescription>SK ID-card external authentication for ASP.NET Core applications.</PackageDescription>
	  <PackageTags>sk;idcard;id-card;authentication</PackageTags>
	  <Authors>kaupov</Authors>
	  <Company>kaupov</Company>
	  <PackageLicenseExpression>MIT</PackageLicenseExpression>
	  <PackageIcon>site_logo.png</PackageIcon>
	  <AddRazorSupportForMvc>true</AddRazorSupportForMvc>
	  <RootNamespace>AspNetCore.Authentication.SK.IDCard</RootNamespace>
	  <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
	  <Product>AspNetCore.Authentication.SK.IDCard</Product>
	  <RepositoryUrl>https://github.com/kaupov/AspNetCore.Authentication.SK.IDCard</RepositoryUrl>
	  <PackageProjectUrl>https://github.com/kaupov/AspNetCore.Authentication.SK.IDCard</PackageProjectUrl>
  </PropertyGroup>

	<ItemGroup>
		<FrameworkReference Include="Microsoft.AspNetCore.App" />
	</ItemGroup>

	<ItemGroup>
	  <PackageReference Include="Microsoft.AspNetCore.Authentication.Certificate" Version="3.1.10" />
	  <PackageReference Include="Rfc2253" Version="1.0.0" />
	</ItemGroup>

	<ItemGroup>
	  <None Include="Images\site_logo.png" Pack="true" PackagePath="\"></None>
	</ItemGroup>
</Project>
